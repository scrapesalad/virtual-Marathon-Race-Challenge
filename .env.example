# Database
DATABASE_URL="postgresql://user:password@host:5432/database?schema=public"

# Next Auth
NEXTAUTH_URL="http://localhost:3000"
NEXTAUTH_SECRET="your-nextauth-secret"

# Strava API
STRAVA_CLIENT_ID=your_client_id
STRAVA_CLIENT_SECRET=your_client_secret
NEXT_PUBLIC_STRAVA_CLIENT_ID="your_client_id"
NEXT_PUBLIC_STRAVA_REDIRECT_URI="http://localhost:3000/api/auth/strava/callback"
NEXT_PUBLIC_STRAVA_MOBILE_REDIRECT_URI="your_mobile_redirect_uri"
NEXT_PUBLIC_APP_URL="http://localhost:3000"

# Mapbox
NEXT_PUBLIC_MAPBOX_ACCESS_TOKEN="your_mapbox_token"

# Supabase
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Email Provider
EMAIL_SERVER_HOST="smtp.example.com"
EMAIL_SERVER_PORT=587
EMAIL_SERVER_USER="your_email"
EMAIL_SERVER_PASSWORD="your_password"
EMAIL_FROM="your_email"

# Cross-Origin
NEXT_PUBLIC_ALLOWED_ORIGINS=http://localhost:3000,your_other_origins 