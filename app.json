{
  "expo": {
    "name": "Virtual Race",
    "slug": "virtual-race",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./public/favicon.ico",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./public/favicon.ico",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.virtualrace.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./public/favicon.ico",
        "backgroundColor": "#ffffff"
      },
      "package": "com.virtualrace.app"
    },
    "web": {
      "favicon": "./public/favicon.ico"
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "virtualrace"
  }
} 